  <div class="container-fluid">
    <div class="col-md-12 search-bar" align="center">

      <form class="form-inline" action="/search" method="post">


        <div class="form-group">
          <i class="fa fa-search" aria-hidden="true"></i>
          <input type="text" class="form-control" id="search" name="search" placeholder="Search e.g. Ruby">
        </div>

      </form>

      <form class="form-inline" action="/wheel" method="post">

        <div class="form-group">
          <select class="select" id="tag" name="tag" onchange="this.form.submit();">
            <option hidden>Filter</option>
            <option name="language" value="language">Language</option>
            <option name="framework" value="framework">Framework</option>
            <option name="package" value="package">Package</option>
            <option name="tool" value="tool">Tool</option>
            <div class="select__arrow"></div>
          </select>
          <i class="fa fa-filter" aria-hidden="true"></i>
        </div>

      </form>

      <a href="/" class="clear-filter"><i class="fa fa-undo" aria-hidden="true"></i>Clear filter</a>
    </div>

  </div>

